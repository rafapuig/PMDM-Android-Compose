<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PMDMCompose">
        <activity
            android:name=".level.LevelActivity"
            android:exported="true"
            android:label="@string/title_activity_level"
            android:theme="@style/Theme.PMDMCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".dashboard_animated_chart.DashBoardAnimatedChartActivity"
            android:exported="true"
            android:label="@string/title_activity_dashboard_animated_chart"
            android:theme="@style/Theme.PMDMCompose"></activity>
        <activity
            android:name=".dashboard_chart.ui.DashBoardChartActivity"
            android:exported="true"
            android:label="@string/title_activity_dashboard_chart"
            android:theme="@style/Theme.PMDMCompose">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".dashboard.ui.DashBoardActivity"
            android:exported="true"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.PMDMCompose"></activity>
        <activity
            android:name=".sensors_data.ui.SensorsActivity"
            android:exported="true"
            android:label="@string/title_activity_sensors"
            android:theme="@style/Theme.PMDMCompose"></activity>
    </application>

</manifest>